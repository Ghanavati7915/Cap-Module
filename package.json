{
  "name": "cap-module",
  "description": "The functions developed by Cap company to establish a connection with the server and also use the services of OAuth2 And IndexedDB",
  "version": "0.0.30",
  "repository": {
    "type": "git",
    "url": "https://github.com/Ghanavati7915/Cap-Module.git"
  },
  "homepage": "https://github.com/Ghanavati7915",
  "bugs": "https://github.com/Ghanavati7915/Cap-Module/issues",
  "changelog": {
    "repo": {
      "provider": "github",
      "owner": "Ghanavati7915",
      "name": "Cap-Module"
    }
  },
  "license": "MIT",
  "type": "module",
  "keywords": [
    "cap",
    "oauth2",
    "axios",
    "indexedDB",
    "iran",
    "ghanavati",
    "etezadi",
    "nuxt",
    "nuxt3",
    "nuxt-module",
    "capModule",
    "cap_module",
    "cap-module"
  ],
  "author": "Ahmad Ghanavati",
  "exports": {
    ".": {
      "types": "./dist/types.d.ts",
      "import": "./dist/module.mjs",
      "require": "./dist/module.cjs"
    }
  },
  "main": "./dist/module.cjs",
  "types": "./dist/types.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "prepack": "nuxt-module-build build",
    "dev": "nuxi dev playground",
    "dev:build": "nuxi build playground",
    "dev:generate": "nuxi generate playground",
    "dev:prepare": "nuxt-module-build build --stub && nuxt-module-build prepare && nuxi prepare playground",
    "release": "npm run prepack && changelogen --release && npm publish && git push --follow-tags",
    "releaseWithTest": "npm run test && npm run prepack && changelogen --release && npm publish && git push --follow-tags",
    "releaseWithLint": "npm run lint && npm run test && npm run prepack && changelogen --release && npm publish && git push --follow-tags",
    "publish": "npm publish",
    "lint": "eslint .",
    "test": "vitest run",
    "test:watch": "vitest watch",
    "test:types": "vue-tsc --noEmit && cd playground && vue-tsc --noEmit"
  },
  "dependencies": {
    "@nuxt/kit": "^3.11.2",
    "axios": "^1.7.2"
  },
  "devDependencies": {
    "@nuxt/devtools": "^1.2.0",
    "@nuxt/eslint-config": "^0.3.10",
    "@nuxt/module-builder": "^0.6.0",
    "@nuxt/schema": "^3.11.2",
    "@nuxt/test-utils": "^3.12.1",
    "@types/node": "^20.12.11",
    "changelogen": "^0.5.5",
    "eslint": "^9.2.0",
    "nuxt": "^3.11.2",
    "typescript": "latest",
    "vitest": "^1.6.0",
    "vue-tsc": "^2.0.16"
  }
}
